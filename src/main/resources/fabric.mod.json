{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_author}"
	],
	"contact": {
		"homepage": "${mod_website}",
		"sources": "${mod_website}",
		"issues": "${mod_website}/issues"
	},
	"license": "${mod_license}",
	"icon": "assets/default-gamemode/icon.png",
